{
  "name": "photogabble/clover-coverage",
  "description": "Command line parser of clover.xml code coverage.",
  "keywords": [
    "clover",
    "code coverage",
    "php"
  ],
  "support": {
    "issues": "https://github.com/photogabble/clover-coverage-cli/issues",
    "source": "https://github.com/photogabble/clover-coverage-cli"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Simon Dann",
      "email": "simon.dann@gmail.com",
      "homepage": "https://www.photogabble.co.uk"
    }
  ],
  "require": {
    "php": ">=5.6",
    "symfony/console": "~2.8|3.*|4.*|5.*",
    "ext-simplexml": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*|6.*|7.*"
  },
  "autoload": {
    "psr-4": {
      "Photogabble\\CloverCoverage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Photogabble\\CloverCoverage\\Tests\\": "tests/"
    }
  },
  "bin": [
    "bin/clover-coverage"
  ]
}
